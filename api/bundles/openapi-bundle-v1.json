{
  "openapi": "3.0.0",
  "info": {
    "title": "Aravica api v1",
    "version": "1.0",
    "description": "Aravica api definitions called from frontend"
  },
  "servers": [
    {
      "description": "development",
      "url": "http://localhost:33000/api/v1"
    }
  ],
  "paths": {
    "/user/user_registrations": {
      "post": {
        "summary": "Create user registrations",
        "operationId": "create-user-registrations",
        "description": "Create user registrations",
        "tags": [
          "user"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "title": "create_user_registrations_request",
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email"
                  }
                },
                "required": [
                  "email"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "create_user_registrations_response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "create_user_registrations_response",
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "format": "int64"
                    },
                    "email": {
                      "type": "string",
                      "format": "email"
                    },
                    "provider": {
                      "type": "string"
                    },
                    "created_at": {
                      "type": "string",
                      "format": "date-time"
                    },
                    "updated_at": {
                      "type": "string",
                      "format": "date-time"
                    }
                  },
                  "required": [
                    "id",
                    "email",
                    "provider",
                    "created_at",
                    "updated_at"
                  ]
                }
              }
            }
          },
          "default": {
            "$ref": "#/components/responses/default"
          }
        }
      }
    },
    "/common/origin_countries": {
      "get": {
        "summary": "List origin countries",
        "operationId": "list-origin-countries",
        "description": "List origin countries",
        "tags": [
          "common"
        ],
        "responses": {
          "200": {
            "description": "list_origin_countries_response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "list_origin_countries_response",
                  "type": "object",
                  "properties": {
                    "results": {
                      "type": "array",
                      "items": {
                        "title": "OriginCountry",
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer",
                            "format": "int64",
                            "minimum": 1
                          },
                          "name": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "id",
                          "name"
                        ]
                      }
                    }
                  },
                  "required": [
                    "results"
                  ]
                }
              }
            }
          },
          "default": {
            "$ref": "#/components/responses/default"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ErrorResponse": {
        "title": "ErrorResponse",
        "type": "object",
        "x-tags": [
          "errors"
        ],
        "properties": {
          "messages": {
            "type": "object"
          },
          "full_messages": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "messages",
          "full_messages"
        ]
      }
    },
    "responses": {
      "default": {
        "description": "default error",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/ErrorResponse"
            }
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "common"
    },
    {
      "name": "user"
    }
  ]
}
