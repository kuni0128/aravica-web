{
  "openapi": "3.0.0",
  "info": {
    "title": "Aravica api v1",
    "version": "1.0",
    "description": "Aravica api definitions called from frontend"
  },
  "servers": [
    {
      "description": "development",
      "url": "/v1"
    }
  ],
  "paths": {
    "/origin-countries": {
      "get": {
        "summary": "List origin countries",
        "operationId": "list-origin-countries",
        "description": "List origin countries",
        "tags": [
          "common"
        ],
        "responses": {
          "200": {
            "description": "list_origin_countries_response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "list_origin_countries_response",
                  "type": "object",
                  "properties": {
                    "results": {
                      "type": "array",
                      "items": {
                        "title": "OriginCountry",
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer",
                            "format": "int64",
                            "minimum": 1
                          },
                          "name": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "id",
                          "name"
                        ]
                      }
                    }
                  },
                  "required": [
                    "results"
                  ]
                }
              }
            }
          },
          "default": {
            "$ref": "#/components/responses/default"
          }
        }
      }
    }
  },
  "components": {
    "responses": {
      "default": {
        "description": "error",
        "content": {
          "application/json": {
            "schema": {
              "title": "ErrorResponse",
              "type": "object",
              "properties": {
                "messages": {
                  "type": "object"
                },
                "full_messages": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              },
              "required": [
                "messages",
                "full_messages"
              ]
            }
          }
        }
      }
    }
  }
}
